[credential]
	helper = 
	helper = store --file /opt/dev/var/private/git_credential_store
[alias]
        ft = fetch
        cm = commit
        co = checkout
        br = branch
        st = status
        po = push origin
        plo = pull origin
        lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[core]
	commitGraph = true
[diff]
	algorithm = patience
[gc]
	writeCommitGraph = true
[protocol]
	version = 2
